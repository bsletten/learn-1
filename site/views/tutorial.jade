html
  head
    meta(charset='utf8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
    meta(name='description', content='Stardog tutorials for learning a great graph awesome!')
    meta(name='author', content= "Complexible Inc")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(name="apple-touch-icon" href="apple-touch-icon.png")
    title
      block title 
    != css("/")
    script(src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js")

  body
    div.theader
     div.toplogo 
       #[a(href="http://stardog.com/") #[img(id="topdog",width="217",height="157",src="/img/stardog.png")]]
     h1.tutorial-title #[a(href="/") Stardog Tutorial]
     h2.tutorial-subtitle
       block subtitle
     h3.version-date
       block version-date
     div.topdownload
       +stardog
    block content

  div.footer
     div.toplogo #[a(href="http://complexible.com/") #[img(id="bottomdog", height="150",width="180",src="/img/l-b.png")] ]
     p.small &copy; 2015 #[a(href="http://complexible.com/") Complexible Inc]. #[a(href="http://creativecommons.org/licenses/by-sa/3.0/") Some rights reserved]. Icons #[a(href="http://www.freepik.com/") designed by Freepik].
   
  != js()
